<div class="header">
  <h5><%= link_to 'Zeus News', articles_path %></h5>
  <h5><%=  link_to 'New Article', new_article_path%></h5>
</div>

<div class="container">
  <% @articles.each do |article| %>
    <h2><%= link_to article.title, article_path(article.id) %></h2>
    <p><%= article.content %></p>
  <% end %>
</div>